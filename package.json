{
  "name": "webpack-react-project",
  "version": "0.0.2",
  "scripts": {
    "start": "webpack-dev-server --port 3000 --inline --progress --colors  --open",
    "server": "node ./server.js",
    "watch": "webpack --config webpack.config.js -w --progress --colors",
    "build": "rm -rf ./dist/* && NODE_ENV=production webpack --config webpack.config.js -p --progress --colors"
  },
  "keywords": [
    "React",
    "React-Router",
    "Webpack",
    "Webpack3",
    "ES6",
    "multiple",
    "多页面"
  ],
  "author": "comid",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.8.5",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.17.0",
    "image-webpack-loader": "^3.4.2",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "react-hot-loader": "^1.3.0",
    "react-router": "^3.2.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "jquery": "^2.2.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.2",
    "react-dom": "^15.4.1"
  }
